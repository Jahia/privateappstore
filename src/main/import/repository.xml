<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0"
         xmlns:jcr="http://www.jcp.org/jcr/1.0"
         xmlns:jmix="http://www.jahia.org/jahia/mix/1.0"
         xmlns:jnt="http://www.jahia.org/jahia/nt/1.0">
    <modules jcr:primaryType="jnt:modules">
        <forge j:dependencies="bootstrapComponent default facets grid rating siteSettings templates-system videostreaming"
             j:installedModules="forge"
             j:moduleType="module"
             j:title="Jahia Forge"
             jcr:primaryType="jnt:module">
                
                <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder"/>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <forge-home j:hiddenTemplate="false"
                        jcr:mixinTypes="jmix:accessControlled"
                        jcr:primaryType="jnt:pageTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <bootstrap-row customColumn="3,9"
                                 divID="jnt_forge"
                                 fluid="true"
                                 jcr:mixinTypes="jmix:accessControlled"
                                 jcr:primaryType="jnt:bootstrapRow">
              <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                <bootstrap-row-col1 jcr:primaryType="jnt:contentList">
                                    <facets-list j:bindedComponent="/modules/forge/contents/forge-modules-repository"
                                     j:type="jnt:forgeModule"
                                     j:view="forge"
                                     jcr:mixinTypes="jmix:accessControlled jmix:renderable"
                                     jcr:primaryType="jnt:facets">
                  <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                        <field-facet facet="Category"
                                        field="jnt:forgeModule;category"
                                        jcr:mixinTypes="jmix:accessControlled"
                                        jcr:primaryType="jnt:fieldFacet"
                                        limit="100"
                                        mincount="1"
                                        missing="false"
                                        offset="0">
                    <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                            <j:translation_en jcr:language="en"
                                                jcr:primaryType="jnt:translation"
                                                label="Category"/>
                  </field-facet>
                                        <field-facet-1 facet="Tags"
                                          field="jmix:tagged;j:tags"
                                          jcr:mixinTypes="jmix:accessControlled"
                                          jcr:primaryType="jnt:fieldFacet"
                                          limit="100"
                                          mincount="1"
                                          missing="false"
                                          offset="0">
                    <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" label="Tags"/>
                  </field-facet-1>
                </facets-list>
                                    <forge-my-modules-list j:skin="skins.bootstrap-rounded-title"
                                               j:style="grey"
                                               jcr:mixinTypes="jmix:accessControlled jmix:skinnable jmix:style"
                                               jcr:primaryType="jnt:forgeMyModulesList">
                  <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                        <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="My Modules"/>
                </forge-my-modules-list>
              </bootstrap-row-col1>
                                <bootstrap-row-col2 jcr:primaryType="jnt:contentList">
                                    <forgeModulesList columnsNumber="3"
                                          jcr:mixinTypes="jmix:accessControlled"
                                          jcr:primaryType="jnt:forgeModulesList"
                                          nbOfModulePerPage="20"
                                          startNode="/modules/forge/contents/forge-modules-repository">
                  <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                </forgeModulesList>
                                    <pager displayNbOfItemsPerPage="true"
                               displayPager="false"
                               j:bindedComponent="/modules/forge/templates/forge-home/pagecontent/bootstrap-row/bootstrap-row-col2/forgeModulesList"
                               jcr:primaryType="jnt:pager"
                               nbOfPages="10"
                               pageSize="3"/>
                                    <file-upload-form j:bindedComponent="/modules/forge/templates/forge-home/pagecontent/bootstrap-row/bootstrap-row-col2"
                                          j:view="jar"
                                          jcr:mixinTypes="jmix:renderable"
                                          jcr:primaryType="jnt:fileUpload"/>
              </bootstrap-row-col2>
            </bootstrap-row>
          </pagecontent>
          <j:acl jcr:primaryType="jnt:acl"/>
                        <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Forge Home"/>
        </forge-home>
                    <forge-module j:applyOn="jnt:forgeModule"
                          j:defaultTemplate="true"
                          j:hiddenTemplate="false"
                          jcr:mixinTypes="jmix:accessControlled"
                          jcr:primaryType="jnt:contentTemplate">
          <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                        <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Forge Module"/>
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <bootstrap-row colNames="module-aside, module-main"
                                 customColumn="3,9"
                                 divID="jnt_forge"
                                 fluid="true"
                                 jcr:mixinTypes="jmix:accessControlled"
                                 jcr:primaryType="jnt:bootstrapRow">
              <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                <module-aside jcr:primaryType="jnt:contentList">
                                    <main-resource-display j:mainResourceView="header"
                                               jcr:mixinTypes="jmix:accessControlled"
                                               jcr:primaryType="jnt:mainResourceDisplay">
                  <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                </main-resource-display>
                                    <developer-panel j:mainResourceView="developerPanel"
                                         jcr:mixinTypes="jmix:accessControlled"
                                         jcr:primaryType="jnt:mainResourceDisplay">
                  <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                </developer-panel>
                                    <forge-admin-panel j:mainResourceView="forgeAdminPanel"
                                           jcr:mixinTypes="jmix:accessControlled"
                                           jcr:primaryType="jnt:mainResourceDisplay">
                  <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                </forge-admin-panel>
              </module-aside>
                                <module-main jcr:primaryType="jnt:contentList">
                                    <bootstrap-tabular-list fadeIn="true"
                                                jcr:mixinTypes="jmix:accessControlled"
                                                jcr:primaryType="jnt:bootstrapTabularList"
                                                tabsPosition="top">
                  <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                        <overview jcr:mixinTypes="jmix:accessControlled"
                                     jcr:primaryType="jnt:contentList">
                    <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="Overview"/>
                                            <bootstrap-row colNames="overview-main,overview-aside"
                                             customColumn="8,3"
                                             fluid="true"
                                             jcr:mixinTypes="jmix:accessControlled"
                                             jcr:primaryType="jnt:bootstrapRow"
                                             offset="0,1">
                      <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                                <overview-main jcr:primaryType="jnt:contentList">
                                                    <main-resource-display j:mainResourceView="overview"
                                                           jcr:mixinTypes="jmix:accessControlled"
                                                           jcr:primaryType="jnt:mainResourceDisplay">
                          <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                        </main-resource-display>
                      </overview-main>
                                                <overview-aside jcr:primaryType="jnt:contentList">
                                                    <main-resource-display j:mainResourceView="information"
                                                           jcr:mixinTypes="jmix:accessControlled"
                                                           jcr:primaryType="jnt:mainResourceDisplay">
                          <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                        </main-resource-display>
                      </overview-aside>
                    </bootstrap-row>
                  </overview>
                                        <screenshots jcr:mixinTypes="jmix:accessControlled"
                                        jcr:primaryType="jnt:contentList">
                    <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="Screenshots"/>
                                            <main-resource-display j:mainResourceView="screenshots"
                                                     jcr:mixinTypes="jmix:accessControlled"
                                                     jcr:primaryType="jnt:mainResourceDisplay">
                      <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    </main-resource-display>
                                            <file-upload-form j:invertCondition="false"
                                                j:requireLoggedUser="true"
                                                j:requirePrivilegedUser="false"
                                                j:requiredPermissions="#/permissions/repository-permissions/jcr:all_live"
                                                j:view="forge"
                                                jcr:mixinTypes="jmix:accessControlled jmix:renderable jmix:requiredPermissions"
                                                jcr:primaryType="jnt:fileUpload">
                      <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    </file-upload-form>
                  </screenshots>
                                        <video jcr:mixinTypes="jmix:accessControlled"
                                  jcr:primaryType="jnt:contentList">
                    <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="Video"/>
                                            <main-resource-display j:mainResourceView="video"
                                                     jcr:mixinTypes="jmix:accessControlled"
                                                     jcr:primaryType="jnt:mainResourceDisplay">
                      <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    </main-resource-display>
                  </video>
                                        <reviews jcr:mixinTypes="jmix:accessControlled"
                                    jcr:primaryType="jnt:contentList">
                    <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="Reviews"/>
                                            <rich-text jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:bigText">
                      <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                                <j:translation_en jcr:language="en"
                                                   jcr:primaryType="jnt:translation"
                                                   text="&lt;h2 class='reviewsHeader'&gt;  User reviews&lt;/h2&gt; "/>
                    </rich-text>
                                            <addreview commentMandatory="false"
                                         commentMinLength="50"
                                         jcr:mixinTypes="jmix:accessControlled"
                                         jcr:primaryType="jnt:addReview">
                      <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    </addreview>
                                            <rating-split j:view="ratingsSplit"
                                            jcr:mixinTypes="jmix:accessControlled jmix:renderable"
                                            jcr:primaryType="jnt:reviewsList">
                      <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    </rating-split>
                                            <reviews-list jcr:mixinTypes="jmix:accessControlled"
                                            jcr:primaryType="jnt:reviewsList">
                      <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    </reviews-list>
                  </reviews>
                                        <installation jcr:mixinTypes="jmix:accessControlled"
                                         jcr:primaryType="jnt:contentList">
                    <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="Installation"/>
                                            <main-resource-display j:mainResourceView="howToInstall"
                                                     jcr:mixinTypes="jmix:accessControlled"
                                                     jcr:primaryType="jnt:mainResourceDisplay">
                      <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    </main-resource-display>
                  </installation>
                                        <faq jcr:mixinTypes="jmix:accessControlled"
                                jcr:primaryType="jnt:contentList">
                    <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="FAQ"/>
                                            <main-resource-display j:mainResourceView="FAQ"
                                                     jcr:mixinTypes="jmix:accessControlled"
                                                     jcr:primaryType="jnt:mainResourceDisplay">
                      <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    </main-resource-display>
                  </faq>
                                        <changelog jcr:mixinTypes="jmix:accessControlled"
                                      jcr:primaryType="jnt:contentList">
                    <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="Changelog"/>
                                            <main-resource-display j:mainResourceView="changeLog"
                                                     jcr:mixinTypes="jmix:accessControlled"
                                                     jcr:primaryType="jnt:mainResourceDisplay">
                      <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    </main-resource-display>
                  </changelog>
                </bootstrap-tabular-list>
              </module-main>
            </bootstrap-row>
          </pagecontent>
        </forge-module>
                    <forge-module-version j:applyOn="jnt:forgeModuleVersion"
                                  j:defaultTemplate="true"
                                  j:hiddenTemplate="false"
                                  j:priority="100"
                                  jcr:mixinTypes="jmix:accessControlled"
                                  jcr:primaryType="jnt:contentTemplate">
          <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <predefined--rows-960gs column="1col16"
                                          jcr:mixinTypes="jmix:accessControlled"
                                          jcr:primaryType="jnt:row">
              <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                <predefined--rows-960gs-col1 jcr:primaryType="jnt:contentList">
                                    <main-resource-display j:mainResourceView="default"
                                               jcr:mixinTypes="jmix:accessControlled"
                                               jcr:primaryType="jnt:mainResourceDisplay">
                  <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                </main-resource-display>
              </predefined--rows-960gs-col1>
            </predefined--rows-960gs>
          </pagecontent>
                        <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Forge Module Version"/>
        </forge-module-version>
                    <siteSettingsBase j:hiddenTemplate="false"
                              j:view="siteSettings"
                              jcr:primaryType="jnt:template">
                        <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="siteSettingsBase"/>
                        <forge j:applyOn="jnt:virtualsite"
                      j:defaultTemplate="false"
                      j:hiddenTemplate="true"
                      j:view="siteSettings"
                      jcr:primaryType="jnt:contentTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="forge"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <forgesettings jcr:primaryType="jnt:forgeSettings"/>
                            </pagecontent>
                        </forge>
                    </siteSettingsBase>
      </templates>
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder">
                    <forge-modules-repository jcr:mixinTypes="jmix:accessControlled"
                                      jcr:primaryType="jnt:contentFolder">
                        <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                        <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Forge Modules Repository"/>
                    </forge-modules-repository>
                    <forge-modules-categories jcr:mixinTypes="jmix:accessControlled"
                                      jcr:primaryType="jnt:contentFolder">
                        <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                        <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Forge Modules Categories"/>
                        <games jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                            <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                            <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" text="Games"/>
                        </games>
                        <social jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                            <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                            <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" text="Social"/>
                        </social>
                    </forge-modules-categories>
                    <forge-modules-required-versions jcr:mixinTypes="jmix:accessControlled"
                                             jcr:primaryType="jnt:contentFolder">
                        <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                        <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Forge Modules-Required Versions"/>
                        <version-6_6 jcr:mixinTypes="jmix:accessControlled"
                            jcr:primaryType="jnt:contentFolder">
                            <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Jahia 6.6"/>
                            <version-6_6_0_0 jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                                <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" text="6.6.0.0"/>
                            </version-6_6_0_0>
                            <version-6_6_0_1 jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                                <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" text="6.6.0.1"/>
                            </version-6_6_0_1>
                        </version-6_6>
                        <version-6_5 jcr:mixinTypes="jmix:accessControlled"
                            jcr:primaryType="jnt:contentFolder">
                            <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Jahia 6.5"/>
                            <version-6_5_0_0 jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                                <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" text="6.5.0.0"/>
                            </version-6_5_0_0>
                            <version-6_5_0_1 jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                                <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                                <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" text="6.5.0.1"/>
                            </version-6_5_0_1>
                        </version-6_5>
                    </forge-modules-required-versions>
                    <forge-modules-release-type jcr:mixinTypes="jmix:accessControlled"
                                        jcr:primaryType="jnt:contentFolder">
                        <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                        <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="forge-modules-release-type"/>
                        <simple-text jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                            <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                            <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" text="Hotfix"/>
                        </simple-text>
                        <simple-text-1 jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                            <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                            <j:translation_en jcr:language="en"
                                    jcr:primaryType="jnt:translation"
                                    text="Service Pack"/>
                        </simple-text-1>
                    </forge-modules-release-type>
                </contents>
                
    </forge>
    </modules>
    
</content>
