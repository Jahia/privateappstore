<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <privateappstore
                j:dependencies="default facets rating siteSettings videostreaming"
                j:installedModules="privateappstore"
                j:modulePriority="0"
                j:moduleType="module"
                j:title="Jahia Private App Store"
                jcr:primaryType="jnt:module">

            <portlets jcr:primaryType="jnt:portletFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder">
                <modules-repository jcr:mixinTypes="jmix:accessControlled"
                                    jcr:primaryType="jnt:contentFolder">
                    <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    <j:translation_en jcr:language="en"
                                      jcr:mixinTypes="mix:title"
                                      jcr:primaryType="jnt:translation"
                                      jcr:title="Private App Store Modules Repository"/>
                </modules-repository>
                <modules-required-versions jcr:mixinTypes="jmix:accessControlled"
                                           jcr:primaryType="jnt:contentFolder">
                    <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    <j:translation_en jcr:language="en"
                                      jcr:mixinTypes="mix:title"
                                      jcr:primaryType="jnt:translation"
                                      jcr:title="Private App Store Modules-Required Versions"/>
                    <version-6_6 jcr:mixinTypes="jmix:accessControlled"
                                 jcr:primaryType="jnt:contentFolder">
                        <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Jahia 6.6"/>
                        <version-6_6_0_0 jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                            <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                            <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" text="6.6.0.0"/>
                        </version-6_6_0_0>
                        <version-6_6_0_1 jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                            <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                            <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" text="6.6.0.1"/>
                        </version-6_6_0_1>
                    </version-6_6>
                    <version-6_5 jcr:mixinTypes="jmix:accessControlled"
                                 jcr:primaryType="jnt:contentFolder">
                        <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                        <j:translation_en jcr:language="en"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Jahia 6.5"/>
                        <version-6_5_0_0 jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                            <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                            <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" text="6.5.0.0"/>
                        </version-6_5_0_0>
                        <version-6_5_0_1 jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                            <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                            <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" text="6.5.0.1"/>
                        </version-6_5_0_1>
                    </version-6_5>
                </modules-required-versions>
                <modules-release-type jcr:mixinTypes="jmix:accessControlled"
                                      jcr:primaryType="jnt:contentFolder">
                    <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                    <j:translation_en jcr:language="en"
                                      jcr:mixinTypes="mix:title"
                                      jcr:primaryType="jnt:translation"
                                      jcr:title="modules-release-type"/>
                    <simple-text jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                        <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                        <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" text="Hotfix"/>
                    </simple-text>
                    <simple-text-1 jcr:mixinTypes="jmix:accessControlled" jcr:primaryType="jnt:text">
                        <j:acl j:inherit="true" jcr:primaryType="jnt:acl"/>
                        <j:translation_en jcr:language="en"
                                          jcr:primaryType="jnt:translation"
                                          text="Service Pack"/>
                    </simple-text-1>
                </modules-release-type>
            </contents>
            <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
            </templates>

        </privateappstore>
    </modules>

</content>
