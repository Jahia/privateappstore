<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">

<urlrewrite>
    <rule>
        <name>RSS feed</name>
        <from>^/cms/render/live/(\w{2})/sites/(.+?)/feed$</from>
        <to>/cms/render/live/$1/sites/$2//contents/modules-repository.moduleList.rss</to>
    </rule>
    <rule>
        <name>RSS feed.xml</name>
        <from>^/cms/render/live/(\w{2})/sites/(.+?)/feed.xml$</from>
        <to>/cms/render/live/$1/sites/$2//contents/modules-repository.moduleList.rss</to>
    </rule>
</urlrewrite>
